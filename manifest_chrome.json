{
    "name": "WMOpenOriginal",
    "description": "Open original page on wayback machine when trigger",
    "version": "1.1",
    "manifest_version": 3,
    "action": {
        "default_title": "WMOpenOriginal",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
          "js": ["content.js"],
          "matches": ["https://web.archive.org/web/*"]
    }],
    "permissions":["storage"]
}
